<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="trackingDisable"
        android:summaryOff="@string/trackingOff"
        android:summaryOn="@string/trackingOn"
        android:title="@string/tracking" />

    <Preference
        android:key="donate"
        android:summary="@string/donateSummary"
        android:title="@string/donate" />

    <Preference
        android:key="app_version"
        android:title="@string/app_version" />

    <Preference
        android:key="email"
        android:summary="@string/email"
        android:title="@string/developer" />
</PreferenceScreen>